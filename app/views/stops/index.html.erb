<table>
  <th>
      <tr>
        <th>Stop Number</th>
        <th>Name</th>
        <th>Description</th>
        <th>Location</th>
        <th>Directions to next stop</th>
        <th>Travel tip</th>
        <th>GPS longitude</th>
        <th>GPS latitude</th>
        <th>Learn_more_URL</th>
        <th>Image Current</th>
        <th>Image Historic</th>
        <th>Badge</th>
      </tr>
    </thread>
    <tbody>
      <% @stops.each do |stop| %>
        <tr>
          <td><%= stop.stop_num %></td>
          <td><%= stop.name %></td>
          <td><%= stop.description %></td>
          <td><%= stop.location %></td>
          <td><%= stop.directions_to_next_stop %></td>
          <td><%= stop.travel_tip %></td>
          <td><%= stop.gps_long %></td>
          <td><%= stop.gps_lat %></td>
          <td><%= stop.learn_more_URL %></td>
          <td><%= stop.image_current %></td>
          <td><%= stop.image_historic %></td>
          <td><%= stop.badge %></td>
        </tr>
      <% end %>
    </tbody>
</table>
<div>
<h3>Import a CSV File</h3>
  <%= form_tag import_tour_stops_path, multipart: true do %>
    <%= file_field_tag :file %>
    <%= submit_tag "Import CSV" %>
  <% end %>
</div>
